import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        long min = Long.MAX_VALUE;
        long max = Long.MIN_VALUE;
        
        for (int i = 0; i < n; i++) {
            long x = sc.nextLong();
            if (x < min) min = x;
            if (x > max) max = x;
        }
        
        System.out.println(min + " " + max);
    }
}
